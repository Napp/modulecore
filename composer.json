{
  "name": "napp/modulecore",
  "description": "Module core for projects",
  "keywords": [
    "napp",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Module\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Module\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.3",
    "illuminate/routing": "~5.6.0|~5.7.0",
    "illuminate/support": "~5.6.0|~5.7.0",
    "napp/aclcore": "~1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.7",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit" : "^7.0",
    "phpunit/phpcov": "^5.0",
    "squizlabs/php_codesniffer": "^3.1",
    "orchestra/testbench": "^3.6",
    "orchestra/database": "^3.6",
    "fzaninotto/faker": "^1.7"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Module\\Provider\\CoreServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
